<!doctype html>
<html>
	<head>
	  <meta charset="UTF-8" />
		<title>Suite</title>
		<style type="text/css" media="screen">@import "/stylesheets/jqt/jqtouch.css";</style>
    <style type="text/css" media="screen">@import "/stylesheets/jqt/themes/jqt/theme.css";</style>
    <script src="/javascripts/jquery.1.3.2.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="/javascripts/jqtouch.js" type="application/x-javascript" charset="utf-8"></script>
    <script type="text/javascript" charset="utf-8">
        $.jQTouch({
            icon: '/sabretech.png',
            addGlossToIcon: true,
            startupScreen: '/suite_loader.png',
            statusBar: 'black',
            preloadImages: [
                '/stylesheets/jqt/themes/jqt/img/back_button.png',
                '/stylesheets/jqt/themes/jqt/img/back_button_clicked.png',
                '/stylesheets/jqt/themes/jqt/img/button_clicked.png',
                '/stylesheets/jqt/themes/jqt/img/grayButton.png',
                '/stylesheets/jqt/themes/jqt/img/whiteButton.png',
                '/stylesheets/jqt/themes/jqt/img/loading.gif'
                ]
        });
    </script>
    <style>
      h2.name {font: bold 18px Helvetica;text-shadow: rgba(255,255,255,.2) 0 1px 1px;color: #ccc;margin: 10px 20px 5px;}
      h3 {font: bold 12px Helvetica;text-shadow: rgba(255,255,255,.2) 0 1px 1px;color: #000;margin: 5px 20px 8px;}
      li.paragraph {font-size:12px;color:#fff;}
      ul li span.description {font-size:13px;color:#eee;}
      ul li table {font-size:11px;color:#999;}
      div#ticket li {position:relative}
      div#ticket li img {position:absolute;}
      div#ticket li span.byline {position:absolute;left:70px;top:10px;font-size:11px;color:#ccc;}
      div#ticket li span.te_details {position:absolute;left:70px;top:33px;font-size:11px;color:#ccc;}
      div#ticket li div.te_note {margin:60px 0px 0px;font-size:13px;color:#fff;} 
    </style
	</head>
	<body>
    <%= yield %>
	</body>
</html>